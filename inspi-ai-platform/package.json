{
  "name": "inspi-ai-platform",
  "version": "0.5.5",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3007",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start -p 3007",
    "start:prod": "NODE_ENV=production next start -p 3007",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:auth-cards": "eslint src/app/api/auth src/components/auth src/components/cards/GeneratedCard.tsx src/shared/hooks/useAuth.tsx",
    "lint:monitoring": "eslint src/app/api/monitoring src/app/api/logging src/app/api/payment src/app/api/share/card src/app/api/subscription/quota",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "type-check:core": "tsc --project tsconfig.core.json --noEmit",
    "type-check:auth-cards": "tsc --project tsconfig.auth-cards.json --noEmit",
    "clean": "rm -rf .next out dist coverage",
    "clean:all": "npm run clean && rm -rf node_modules && npm install",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "pre-commit": "npm run validate && npm run test:unit",
    "test": "jest",
    "test:unit": "jest --config jest.config.unit.js",
    "test:integration": "jest --config jest.config.integration.js",
    "test:e2e": "playwright test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:all --ci --coverage --watchAll=false",
    "build:verify": "npm run validate && npm run build",
    "build:production": "npm run validate && npm run test:unit && npm run build",
    "dev:check": "npm run type-check && npm run lint",
    "dev:test": "npm run test:watch",
    "quality:check": "node scripts/quality-check.js",
    "quality:lint": "npm run lint && npm run type-check",
    "quality:test": "npm run test:coverage",
    "quality:all": "npm run quality:lint && npm run quality:test && npm run quality:check",
    "quality:report": "npm run quality:all && echo 'Quality report generated'",
    "version:bump": "node scripts/bump-version.js",
    "version:major": "node scripts/bump-version.js major",
    "version:minor": "node scripts/bump-version.js minor",
    "version:patch": "node scripts/bump-version.js patch",
    "release": "npm run build:production && npm run version:bump",
    "release:patch": "npm run build:production && npm run version:patch",
    "release:minor": "npm run build:production && npm run version:minor",
    "release:major": "npm run build:production && npm run version:major",
    "dev:demo-login": "node scripts/setup-demo-login.js",
    "setup:fonts": "node scripts/download-fonts.js",
    "deps:analyze": "node scripts/analyze-dependencies.js",
    "deps:update": "npm update && npm audit fix",
    "commit:feat": "git add . && git commit -m 'feat: '",
    "commit:fix": "git add . && git commit -m 'fix: '",
    "commit:docs": "git add . && git commit -m 'docs: '",
    "commit:refactor": "git add . && git commit -m 'refactor: '",
    "commit:test": "git add . && git commit -m 'test: '",
    "commit:chore": "git add . && git commit -m 'chore: '"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tiptap/extension-color": "^3.4.4",
    "@tiptap/extension-font-family": "^3.4.4",
    "@tiptap/extension-text-style": "^3.4.4",
    "@tiptap/pm": "^3.4.4",
    "@tiptap/react": "^3.4.4",
    "@tiptap/starter-kit": "^3.4.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/d3": "^7.4.3",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/next-auth": "^3.13.0",
    "@types/nodemailer": "^7.0.1",
    "@types/qrcode": "^1.5.5",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "critters": "^0.0.24",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "dom-to-image": "^2.6.0",
    "framer-motion": "^12.23.12",
    "google-auth-library": "^10.3.0",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "mongoose": "^8.18.0",
    "next": "^14.2.6",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "qrcode": "^1.5.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "redis": "^5.8.2",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.46.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/redis": "^4.0.10",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.6",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "msw": "^2.6.8",
    "next-themes": "^0.4.6",
    "playwright": "^1.46.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "whatwg-fetch": "^3.6.20",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35"
  }
}
